<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.idkotlin.idreminder">
    


    <application
        android:name="com.idkotlin.idreminder.BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity  android:name="com.idkotlin.idreminder.presentation.ui.reminder.ReminderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name="com.idkotlin.idreminder.presentation.receiver.AlarmReceiver">

        </receiver>
    </application>

</manifest>